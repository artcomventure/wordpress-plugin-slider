#. Plugin Name of the plugin
#: slider.php
#: build/block.js:1
msgid "Slider"
msgstr "Slider"

#. Description of the plugin
#: slider.php
#: build/block.js:1
msgid "Swiper goes Gutenberg."
msgstr "Swiper goes Gutenberg."

#. Author of the plugin
#: slider.php
msgid "artcom venture GmbH"
msgstr "artcom venture GmbH"

#. Author URI of the plugin
#: slider.php
msgid "http://www.artcom-venture.de/"
msgstr "http://www.artcom-venture.de/"

#: build/block.js:1
msgid "Slider Settings"
msgstr "Paramètres du curseur"

#: build/block.js:1
msgid "Display"
msgstr "Afficher"

#: build/block.js:1
msgid "Slides per view"
msgstr "Diapositives par vue"

#: build/block.js:1
msgid "Number (or \"auto\") of slides visible at the same time on slider's container."
msgstr "Nombre (ou \"auto\") de diapositives visibles en même temps sur le conteneur du curseur."

#: build/block.js:1
msgid "Space between slides (in px)"
msgstr "Espace entre les diapositives (en px)"

#: build/block.js:1
msgid "Show overflow"
msgstr "Afficher le débordement"

#: build/block.js:1
msgid "Show slides outside of slider's premises."
msgstr "Afficher les diapositives en dehors des locaux du slider."

#: build/block.js:1
msgid "Center slides"
msgstr "Glissières centrales"

#: build/block.js:1
msgid "Active slide will be centered, not always on the left side."
msgstr "La diapositive active sera centrée, pas toujours sur le côté gauche."

#: build/block.js:1
msgid "Auto height"
msgstr "Hauteur automatique"

#: build/block.js:1
msgid "Enable and slider wrapper will adapt its height to the height of the currently active slide."
msgstr "L'activation et le wrapper du curseur adapteront sa hauteur à la hauteur de la diapositive actuellement active."

#: build/block.js:1
msgid "Navigation"
msgstr "La navigation"

#: build/block.js:1
msgid "Aka prev and next buttons."
msgstr "Boutons précédent et suivant."

#: build/block.js:1
msgid "Pagination"
msgstr "Pagination"

#: build/block.js:1
msgid "Nope"
msgstr "Non"

#: build/block.js:1
msgid "Bullets"
msgstr "Balles"

#: build/block.js:1
msgid "Fraction"
msgstr "Fraction"

#: build/block.js:1
msgid "Progressbar"
msgstr "Barre de progression"

#: build/block.js:1
msgid "Scrollbar"
msgstr "Barre de défilement"

#: build/block.js:1
msgid "Animation"
msgstr "Animation"

#: build/block.js:1
msgid "Transition effect"
msgstr "Effet de transition"

#: build/block.js:1
msgid "slide"
msgstr "glissoire"

#: build/block.js:1
msgid "fade"
msgstr "disparaître"

#: build/block.js:1
msgid "cube"
msgstr "cube"

#: build/block.js:1
msgid "coverflow"
msgstr "coverflow"

#: build/block.js:1
msgid "flip"
msgstr "flip"

#: build/block.js:1
msgid "cards"
msgstr "cards"

#: build/block.js:1
msgid "Speed"
msgstr "Vitesse"

#: build/block.js:1
msgid "Duration of transition between slides (in ms)."
msgstr "Durée de transition entre les diapositives (en ms)."

#: build/block.js:1
msgid "Direction"
msgstr "Direction"

#: build/block.js:1
msgid "horizontal"
msgstr "horizontal"

#: build/block.js:1
msgid "vertical"
msgstr "vertical"

#: build/block.js:1
msgid "Behaviour"
msgstr "Comportement"

#: build/block.js:1
msgid "Slides per group"
msgstr "Diapositives par groupe"

#: build/block.js:1
msgid "Set numbers of slides to define and enable group sliding. \"auto\" will slide as many slides as defined in \"Slides per view\"."
msgstr "Définissez le nombre de diapositives à définir et activez le glissement de groupe. \"auto\" fera glisser autant de diapositives que défini dans \"Diapositives par vue\"."

#: build/block.js:1
msgid "Loop"
msgstr "Boucle"

#: build/block.js:1
msgid "Enable continuous loop mode."
msgstr "Activer le mode boucle continue."

#: build/block.js:1
msgid "Rewind"
msgstr "Rembobiner"

#: build/block.js:1
msgid "Last slide will slide back to the first slide and vice versa."
msgstr "La dernière diapositive reviendra à la première diapositive et vice versa."

#: build/block.js:1
msgid "Slide to clicked slide"
msgstr "Faire glisser vers la diapositive cliquée"

#: build/block.js:1
msgid "Click on any slide will produce transition to this slide."
msgstr "Cliquez sur n'importe quelle diapositive pour produire une transition vers cette diapositive."

#: build/block.js:1
msgid "Autoplay"
msgstr "Lecture automatique"

#: build/block.js:1
msgid "Delay between transitions (in ms)."
msgstr "Délai entre les transitions (en ms)."

#: build/block.js:1
msgid "Keyboard control"
msgstr "Contrôle du clavier"

#: build/block.js:1
msgid "Sliders currently in viewport can be controlled using the keyboard."
msgstr "Les curseurs actuellement dans la fenêtre d'affichage peuvent être contrôlés à l'aide du clavier."

#: build/block.js:1
msgid "Simulate touch"
msgstr "Simuler le toucher"

#: build/block.js:1
msgid "Slider will accept mouse events like touch events (click and drag to change slides)."
msgstr "Le curseur acceptera les événements de souris comme les événements tactiles (cliquez et faites glisser pour changer de diapositive)."

#: build/block.js:1
msgid "Mouse wheel"
msgstr "Roulette de la souris"

#: build/block.js:1
msgid "Navigate through slides using mouse wheel."
msgstr "Naviguez dans les diapositives à l'aide de la molette de la souris."

#: build/block.js:1
msgid "Release on edges"
msgstr "Libération sur les bords"

#: build/block.js:1
msgid "Allow page scrolling when swiper is on edge positions (in the beginning or in the end)"
msgstr "Autoriser le défilement de la page lorsque le curseur est sur les positions de bord (au début ou à la fin)"

#: build/block.js:1
msgid "Slide view"
msgstr "Vue de diapositive"

#: build/block.js:1
msgid "List view"
msgstr "Vue de liste"

#: build/block.js:1
msgid "show slide %d"
msgstr "afficher la diapositive %d"

#: build/block.js:1
msgid "Slide"
msgstr "Glissoire"

#: js/slider.js:99
msgid "This is the first slide"
msgstr "Ceci est la première diapositive"

#: js/slider.js:100
msgid "This is the last slide"
msgstr "Ceci est la dernière diapositive"

#: js/slider.js:101
msgid "Next slide"
msgstr "Diapositive suivante"

#: js/slider.js:102
msgid "Previous slide"
msgstr "Diapositive précédente"

#: js/slider.js:103
msgid "Go to slide {{index}}"
msgstr "Accéder à la diapositive {{index}}"

#: build/block.js:1
msgid "Freemode"
msgstr "Mode libre"

#: build/block.js:1
msgid "Accessibility"
msgstr "Accessibilité"

#: build/block.js:1
msgid "Role"
msgstr "Rôle"

#: build/block.js:1
msgid "Value of the \"role\" attribute to be set."
msgstr "Valeur de l'attribut \"role\" à définir."

#: build/block.js:1
msgid "Label"
msgstr "Label"

#: build/block.js:1
msgid "Message for screen readers what this slider is about."
msgstr "Message pour les lecteurs d'écran expliquant à quoi sert ce curseur."
